modules = ["python-3.11", "python3"]

[nix]
channel = "stable-24_05"
packages = ["glibcLocales"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Economic Dashboard Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "T2D Pulse Chart Mockup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Option A Revised Mockup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Option B Revised Mockup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Option C Revised Mockup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Option D Revised Mockup"

[[workflows.workflow]]
name = "Economic Dashboard Server"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python wsgi.py"
waitForPort = 5000

[[workflows.workflow]]
name = "T2D Pulse Chart Mockup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python t2d_pulse_chart.py"
waitForPort = 5050

[[workflows.workflow]]
name = "Option A Revised Mockup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python option_a_revised_mockup.py"
waitForPort = 5001

[[workflows.workflow]]
name = "Option B Revised Mockup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python option_b_revised_mockup.py"
waitForPort = 5002

[[workflows.workflow]]
name = "Option C Revised Mockup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python option_c_revised_mockup.py"
waitForPort = 5003

[[workflows.workflow]]
name = "Option D Revised Mockup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python option_d_revised_mockup.py"
waitForPort = 5004

[deployment]
run = ["sh", "-c", "python wsgi.py"]
deploymentTarget = "cloudrun"

